{
  "name": "reactBootTypeScript",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "sh postinstall",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle-ios": "npx react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle",
    "release-ios-changelog": "yarn install && yarn run generate-changelog && fastlane ios beta",
    "release-android-changelog": "yarn install && yarn run generate-changelog && fastlane android beta",
    "release-ios-changelog-bump": "yarn install && yarn run generate-changelog-bump && fastlane ios beta",
    "release-android-changelog-bump": "yarn install && yarn run generate-changelog-bump && fastlane android beta",
    "generate-changelog": "fastlane changelog build_changelog bump_type:\"patch\"",
    "generate-changelog-bump": "fastlane changelog build_changelog",
    "release-ios": "yarn install && fastlane ios beta",
    "release-android": "yarn install && fastlane android beta",
    "release": "yarn run generate-changelog && yarn run release-android && yarn run release-ios",
    "release-bump": "yarn run generate-changelog-bump && yarn run release-android && yarn run release-ios"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "axios": "^1.2.6",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.3.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localization": "^2.3.1",
    "react-native-localize": "^2.2.4",
    "react-native-mmkv": "^2.6.3",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.29.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": ">=18"
  }
}
